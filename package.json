    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@vue/test-utils": "^1.0.0-beta.31",
    "autoprefixer": "^9.7.4",
    "babel-core": "6.26.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "compression-webpack-plugin": "^3.1.0",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "jest": "^25.1.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.2",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-jest": "^3.0.5",
    "vue-loader": "15.9.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
