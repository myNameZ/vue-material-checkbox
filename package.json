{
  "name": "vue-material-checkbox",
  "description": "Checkbox component for Vue",
  "version": "2.1.0",
  "author": "rei",
  "keywords": [
    "material",
    "checkbox",
    "vue"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xrei/vue-material-checkbox"
  },
  "license": "MIT",
  "main": "dist/vue-material-checkbox.js",
  "browser": {
    "./sfc": "src/vue-material-checkbox.vue"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config docs/webpack.config.js --mode development",
    "docs": "rimraf docs/dist && cross-env NODE_ENV=production webpack --config docs/webpack.config.js --progress --hide-modules --mode production",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --progress --hide-modules --mode production",
    "test": "jest"
  },
  "dependencies": {
    "vue": "^2.6.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/preset-env": "^7.5.5",
    "@vue/test-utils": "^1.0.0-beta.29",
    "autoprefixer": "^9.6.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "compression-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.1.0",
    "jest": "^24.8.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-jest": "^3.0.4",
    "vue-loader": "15.7.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.jsx?$": "babel-jest"
    }
