  "version": "2.2.0",
  "author": "rei",
  "keywords": [
    "material",
    "checkbox",
    "vue"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xrei/vue-material-checkbox"
  },
  "license": "MIT",
  "main": "dist/vue-material-checkbox.js",
  "browser": {
    "./sfc": "src/vue-material-checkbox.vue"
  },
  "scripts": {
